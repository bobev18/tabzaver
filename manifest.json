{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "tabzaver",
    "background": {
        "scripts": ["tabzaver.js"]
    },
    "permissions": ["tabs", "clipboardWrite"],
    "browser_action": {
        "default_icon": {
            "38": "icon38.png"
        },
        "default_title": "Copy tab URLs"
    }
}
